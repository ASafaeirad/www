---
import Items from "../items.astro";

import { concepts } from "@/data/concepts";

const items = concepts.map((concept) => ({
  title: concept.title,
  description: `(${concept.concepts.length} concept${concept.concepts.length !== 1 ? "s" : ""})`,
  url: `/concepts/${concept.slug}`,
}));
---

<Items items={items} />
